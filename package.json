{
	"name": "snowpack-svelte-ts-tw",
	"homepage": "https://github.com/GarrettCannon/snowpack-svelte-ts-tw",
	"description": "Community template for Snowpack + Svelte + Typescript + TailwindCSS",
	"version": "1.3.0",
	"license": "MIT",
	"author": {
		"name": "Garrett Cannon",
		"email": "garrett@gcannon.net"
	},
	"keywords": [
		"csa-template",
		"snowpack",
		"svelte",
		"typescript",
		"tailwindcss"
	],
	"publishConfig": {
